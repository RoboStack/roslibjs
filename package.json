{
  "name": "roslib",
  "homepage": "https://www.robotwebtools.org",
  "description": "The standard ROS Javascript Library",
  "version": "1.0.1",
  "license": "BSD-2-Clause",
  "main": "build/roslib.js",
  "module": "build/roslib.module.js",
  "browser": {
